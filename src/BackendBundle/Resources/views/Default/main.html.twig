<!DOCTYPE html>
<html lang="en">
    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>{% block title %}Agile SCRUM{% endblock %}</title>

        <!-- Bootstrap Core CSS -->
        <link href="{{asset('bundles/backend/css/bootstrap.min.css')}}" rel="stylesheet">

        <!-- jQuery UI CSS -->
        <link href="{{asset('bundles/backend/jquery-ui/jquery-ui.css')}}" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="{{asset('bundles/backend/css/sb-admin.css')}}" rel="stylesheet">

        <!-- Morris Charts CSS -->
        <link href="{{asset('bundles/backend/css/plugins/morris.css')}}" rel="stylesheet">

        <!-- Custom Fonts -->
        <link href="{{asset('bundles/backend/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet" type="text/css">

        <!-- Fancybox Styles -->
        <link rel="stylesheet" href="{{asset('bundles/backend/fancybox/source/jquery.fancybox.css?v=2.1.5')}}" type="text/css" media="screen" />
        <link rel="stylesheet" href="{{ asset('bundles/backend/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5')}}" type="text/css" media="screen" />
        <link rel="stylesheet" href="{{ asset('bundles/backend/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7')}}" type="text/css" media="screen" />
        <!-- End Fancybox Styles -->

        <!-- Custom Styles -->
        <link href="{{asset('bundles/backend/css/style.css')}}" rel="stylesheet" type="text/css">
        
        <!-- Custom Search Bar -->
        <link href="{{asset('bundles/backend/css/advanced-search.css')}}" rel="stylesheet" type="text/css">

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>

    <body>
        {% import 'BackendBundle:Default:macros.html.twig' as filters %}
        <div id="wrapper">
            <!-- Navigation -->
            {% include 'BackendBundle:Default:header.html.twig' %}
            <div id="page-wrapper">
                <div class="container-fluid">
                    {% block content %}
                    {% endblock %}
                </div>
                <!-- /.container-fluid -->
            </div>
            <!-- /#page-wrapper -->
        </div>
        <!-- /#wrapper -->

        <!-- jQuery -->
        <script src="{{ asset('bundles/backend/js/jquery.js') }}"></script>

        <!-- jQuery UI-->
        <script src="{{ asset('bundles/backend/jquery-ui/jquery-ui.js') }}"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="{{ asset('bundles/backend/js/bootstrap.min.js') }}"></script>

        <!-- Bootbox Core JavaScript -->
        <script src="{{ asset('bundles/backend/js/bootbox.js') }}" type="text/javascript"></script>

        <script src="{{ asset('bundles/backend/js/tinymce/tinymce.min.js') }}" type="text/javascript"></script>

        <!-- Funciones de uso comun en la aplicacion -->
        <script src="{{ asset('bundles/backend/js/utilities.js') }}" type="text/javascript"></script>

        <!-- Fancybox JS -->
        <script type="text/javascript" src="{{ asset('bundles/backend/fancybox/lib/jquery.mousewheel-3.0.6.pack.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/backend/fancybox/source/jquery.fancybox.pack.js?v=2.1.5') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/backend/fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5')}} "></script>
        <script type="text/javascript" src="{{ asset('bundles/backend/fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.6')}}"></script>
        <script type="text/javascript" src="{{ asset('bundles/backend/fancybox/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7')}}"></script>
        <!-- End Fancybox JS -->


        <!-- Morris Charts JavaScript -->
        <script src="{{ asset('bundles/backend/js/plugins/morris/raphael.min.js') }}"></script>
        <script src="{{ asset('bundles/backend/js/plugins/morris/morris.min.js') }}"></script>
        <script src="{{ asset('bundles/backend/js/plugins/morris/morris-data.js') }}"></script>

        <!-- Flot Charts JavaScript -->
        <!--[if lte IE 8]><script src="js/excanvas.min.js"></script><![endif]-->
        <script src="{{ asset('bundles/backend/js/plugins/flot/jquery.flot.js') }}"></script>
        <script src="{{ asset('bundles/backend/js/plugins/flot/jquery.flot.tooltip.min.js') }}"></script>
        <script src="{{ asset('bundles/backend/js/plugins/flot/jquery.flot.resize.js') }}"></script>
        <script src="{{ asset('bundles/backend/js/plugins/flot/jquery.flot.pie.js') }}"></script>

        {% block other_scripts %}
        {% endblock %}

        <script type="text/javascript">
            $(document).ready(function () {

                /* Codigo para eventos de fancybox */
                $(".fancybox").fancybox();

                /* Codigo para eventos de popovers */
                $('.popover-menu').click(function (event) {
                    $('.popover-menu').popover('hide');
                    var title = $(this).attr('title');
                    var content = $(this).attr('content');
                    $(this).popover({title: title, content: content, html: true, placement: "auto right"});
                    event.stopPropagation();
                });
                $('.popover').click(function (event) {
                    if ($(this).is(':visible')) {
                        event.stopPropagation();
                    }
                });
                $('html').click(function () {
                    if ($('.popover').is(':visible')) {
                        $('.popover').popover('hide');
                    }
                });
                $('.popover-menu').trigger('click');

                {% block scripts %}
                {% endblock %}
            });
            {% block functions_javascript %}
            {% endblock %}
        </script>
    </body>
</html>
